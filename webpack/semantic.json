    "card",
    "container",
    "divider",
    "grid",
    "header",
    "image",
    "item",
    "list",
    "menu",
    "reveal",
    "segment",
    "site",
    "table",
    "visibility",
    "reset"
  ],
  "version": "2.1.8"
}
